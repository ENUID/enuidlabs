{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/api/save-email",
      "destination": "/api/save-email.js"
    },
    {
      "source": "/api/emails/count",
      "destination": "/api/emails/count.js"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
