{
  "version": 2,
  "buildCommand": "npm install && npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/api/save-email",
      "destination": "/api/save-email.js"
    },
    {
      "source": "/api/emails/count",
      "destination": "/api/emails/count.js"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
