{
  "version": 2,
  "builds": [
    {
      "src": "api/save-email.js",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/save-email",
      "dest": "/api/save-email.js"
    },
    {
      "src": "/api/emails/count",
      "dest": "/api/save-email.js"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "includeFiles": ["wishlist-emails.json"]
}
